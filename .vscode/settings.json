{
    "workbench.iconTheme": "vs-seti",
    "files.autoSave": "off",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "yaml.schemas": {
        "schema.json": "*.yaml"
    },
    "yaml.customTags": [
        "!upload scalar",
        "!remove scalar",
        "!keep scalar"
    ],
    "files.exclude": {
        // "**/.git": true,
        // "**/.svn": true,
        // "**/.hg": true,
        // "**/CVS": true,
        // "**/.DS_Store": true,
        // "**/tslint.json": true,
        // "**/tsfmt.json": true,
        // "**/tsconfig.json": true
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
    }
}